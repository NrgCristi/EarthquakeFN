<Window x:Class="EarthquakeFN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EarthquakeFN"
        mc:Ignorable="d"
        Title="Fortnite Launcher" Height="450" Width="800">
    <Grid x:Name="Fortnite_Launcher">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="193*"/>
            <ColumnDefinition Width="607*"/>
        </Grid.ColumnDefinitions>
        <Image Margin="0,1,0,0" Source="/images.jpg" Stretch="Fill" Grid.ColumnSpan="2"/>
        <TextBox x:Name="PathTextBox" TextWrapping="Wrap" Margin="0,365,0,0" Grid.ColumnSpan="2" Text="Fortnite Path"/>
        <Label Content="Welcome To EarthquakeFN" IsEnabled="True"/>
        <Button Content="Launch" Margin="414,189,0,69" Grid.Column="1" Click="Button_Click"/>

    </Grid>
</Window>
